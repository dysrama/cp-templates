{
      "name": "gcr.io/google.com/cloudsdktool/cloud-sdk:slim",
      "args": ["gcloud", "beta", "builds", "triggers", "create", "github",
               "--name=${_APP_NAME}-trigger",
               "--repo-name=cp-templates",
               "--repo-owner=gitrey",
               "--branch-pattern=main",
               "--build-config=./iac-terraform-cloudbuild/cloudbuild.yaml",
               "--substitutions=_APP_NAME=${_APP_NAME},_BRANCH_NAME=${_BRANCH_NAME}"
              ]
}
